{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
      "plugin:react/recommended",
      "standard",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "react", 
      "react-hooks", 
      "@typescript-eslint", 
      "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },

    "rules": {
        "import/no-unresolved": "error"
    },
    "settings": {
    "import/resolver": {
        "typescript": {}
    }
  }
}
